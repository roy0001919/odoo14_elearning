<odoo>
    <record id="new_version_atlas_view" model="ir.ui.view">
        <field name="name">tokiku.new_version_atlas_view</field>
        <field name="model">tokiku.version_wizard</field>
        <field name="arch" type="xml">
            <form string="New Version Atlas Import">
                <group>
                    <!--<field name="last_design_change_ver" invisible="1"/>-->
                    <field name="design_change_atlas_version"/>
                    <!--<field name="design_change_atlas_version" options="{'always_reload': True}"/>-->
                    <field name="new_version_atlas_reason" placeholder="Please input reason." required="1"/>
                    <!--<field name="new_version_atlas_reason" placeholder="Please input reason."/>-->
                </group>
                <footer>
                    <button name="act_confirm" string="Import" type="object" class="oe_highlight"/> or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
